{
  "name": "mernstack",
  "version": "0.1.1",
  "description": "A Mern Stack project to kick start your apps using Microservice Architecture",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && webpack",
    "build:prod": "npm run clean && webpack -p",
    "clean": "rimraf dist && rimraf node_modules",
    "clean:dist": "rimraf dist",
    "clean:mod": "rimraf node_modules",
    "start": "cross-env NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "test": "mocha --compilers js:babel-core/register",
    "test:set": "cross-env NODE_ENV=test BABEL_ENV=test nodemon --exec mocha --recursive ./test/ --compilers js:babel-core/register",
    "test:watch": "npm run test:set",
    "prod:test": "cross-env NODE_ENV=production node index.js",
    "prod:deploy": "cross-env NODE_ENV=production pm2 start index.js --watch"
  },
  "keywords": [
    "mongodb",
    "expressJS",
    "reactJS",
    "reduxjs",
    "webpack",
    "nodeJS",
    "mern",
    "mern-stack",
    "mernStack"
  ],
  "author": "SOSANA",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/SOSANA/mernStack.git"
  },
  "bugs": {
    "url": "https://github.com/SOSANA/mernStack/issues"
  },
  "homepage": "https://github.com/SOSANA/mernStack",
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "axios": "^0.15.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "compression": "^1.6.2",
    "cross-env": "^3.1.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "morgan": "^1.8.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "3.0.2",
    "redux": "3.6.0",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "nodemon": "^1.11.0",
    "offline-plugin": "^4.6.1",
    "postcss-loader": "^1.3.3",
    "react-hot-loader": "3.0.0-beta.6",
    "redbox-react": "^1.3.4",
    "redux-logger": "^2.8.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "sw-precache-webpack-plugin": "^0.9.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "^1.10.1",
